@model dynamic

@{
    ViewBag.Title = "Orders";
}

@section AltNav
{
    <ul class="nav nav-pills pull-right">
        <li>
            <a href="@Url.Action("Create", "Orders")">Manual Enter</a>
        </li>
    </ul>
}

<div class="row-fluid">
    <div class="span2">
        <div class="">
            <form>
                <input type="text" class="input-medium" placeholder="Order ID">
                <input type="text" class="input-medium" placeholder="Customer Name">
                <input type="text" class="input-medium" placeholder="Customer Email">
                <input type="text" class="input-medium" placeholder="Customer Phone">
            </form>
        </div>
    </div>
    <div class="span10">
        <table class="table table-striped">
            <tr>
                <th>Order ID</th>
                <th>Customer</th>
                <th>Order Date</th>
                <th>Amount</th>
                <th>Status</th>
                <th># of items</th>
                <th style="text-align: right">Amount</th>
            </tr>
            <tr>
                <td>123</td>
                <td>Jane Doe</td>
                <td>1/2/1234</td>
                <td>250</td>
                <td>Active</td>
                <td>12</td>
                <td style="background-color: #efefef;text-align: right">$100.00</td>
            </tr>
            <tr>
                <td colspan="2">
                    <a href="#">download</a>
                </td>
                <td colspan="3"></td>
                <td colspan="1">Summary</td>
                <td style="background-color: #efefef;text-align: right">$100.00</td>
            </tr>
        </table>
    </div>
</div>




<div class="row-fluid">
    <div class="pagination span6 offset2">
